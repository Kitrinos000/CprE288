#include "movment.h"

int main() {

    ///Allocate and clear all memory for OI Struct
    oi_t *sensor = oi_alloc();

    ///Initialize open interface
    oi_init(sensor);

    move_forward(sensor, 200);

    //deletes the sensor information (variable)
    oi_free(sensor);

    return 0;

}
